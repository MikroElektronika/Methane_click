module Click_Methane_adc

include Click_Methane_types

sub procedure methane_adcInit() 
sub function  methane_readADC() as uint32_t 

implements

sub procedure methane_adcInit() 

    ADC1_Init() 
    ADC_Set_Input_Channel(_ADC_CHANNEL_4) 

end sub

sub function  methane_readADC() as uint32_t 
dim
    readADC as uint32_t

    readADC = ADC1_Read(4) 
    result = readADC 

end sub

end.